    "import h5py\n",
    "file = h5py.File(\"../Data/raw_data/DTI_voxel_network.mat\", \"r\")\n",
    "xyz = file[\"dti_xyz2\"][:]\n",
    "print(\"xyz shape\", xyz.shape)\n",
    "from mayavi import mlab\n",
    "x, y, z = xyz[:, 0], xyz[:, 1], xyz[:, 2]\n",
    "mlab.points3d(x, y, z, )\n",
    "mlab.show()"
